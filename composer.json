{
    "name": "wexample/symfony-api",
    "description": "A simple syntax to expose API in Symfony",
    "type": "library",
    "license": "MIT",
    "version": "0.1.24",
    "autoload": {
        "psr-4": {
            "Wexample\\SymfonyApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "weeger",
            "email": "romain.weeger@wexample.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "wexample/symfony-helpers": "^0.1.29",
        "wexample/symfony-design-system": "^0.1.11"
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/console api:check-node-install"
        ],
        "post-update-cmd": [
            "php bin/console api:check-node-install"
        ]
    }
}