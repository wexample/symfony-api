{
    "name": "wexample/symfony-api",
    "description": "A simple syntax to expose API in Symfony",
    "type": "library",
    "license": "MIT",
    "version": "1.0.83",
    "keywords": [
        "symfony-ux"
    ],
    "autoload": {
        "psr-4": {
            "Wexample\\SymfonyApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "weeger",
            "email": "romain.weeger@wexample.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "wexample/symfony-testing": "*",
        "wexample/symfony-helpers": "*",
        "wexample/symfony-loader": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/console api:check-node-install"
        ],
        "post-update-cmd": [
            "php bin/console api:check-node-install"
        ]
    }
}